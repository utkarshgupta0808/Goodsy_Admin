<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goodsy.goodsyadmin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.DeliveryBoyInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.DeliveryBoyListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.OrdersPerUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.OrderPerShopActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.NewDeliveryBoyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ChatListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ChatActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.AddDefaultItemsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.AddDefaultImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ItemInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ShopListItemActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.RejectedShopsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.AcceptedShopsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ShopInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.PhotoPreviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ItemListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ShopListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ForgotPassActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />

        <service
            android:name=".Services.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>